<div class="uk-flex uk-flex-center uk-flex-middle uk-grid-small" uk-grid>
    {{> title-card title="Point of Interest Web App" subTitle="Point of Interest Details"}}
    {{> main-menu }}
</div>
<div class="uk-container uk-margin">
    <div class="uk-child-width-expand uk-flex-center uk-flex-middle uk-text-center" uk-grid>
        <div class="uk-width-expand@m">
            {{> view-poi}}
        </div>
        <div class="uk-width-auto@m">
            <img width="300" src="/images/poi.png">
        </div>
    </div>
</div>
<div class="uk-child-width-1-4@s uk-flex uk-flex-center" uk-grid uk-height-match="target: .uk-card">
    {{#each poi.image}}
        {{> image-card}}
    {{/each}}
</div>
